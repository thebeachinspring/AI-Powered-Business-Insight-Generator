    }
   },
   "cell_type": "code",
   "source": [
    "def generate_exec_summary(pack: dict) -> str:\n",
    "    month = pack[\"Month\"]\n",
    "    sales_change = pack.get(\"Sales_Change_Pct\")\n",
    "    profit_change = pack.get(\"Profit_Change_Pct\")\n",
    "    margin_pt = pack.get(\"Margin_Point_Change\")\n",
    "\n",
    "    lines = []\n",
    "    lines.append(f\"### Executive Summary â€” {month}\")\n",
    "\n",
    "    # Sales\n",
    "    if sales_change is not None:\n",
    "        lines.append(f\"- **Sales** were {pack['Sales_Trend']} (**{sales_change:+.2f}% MoM**) to **${pack['Sales_Current']:,.2f}**.\")\n",
    "    else:\n",
    "        lines.append(f\"- **Sales**: ${pack['Sales_Current']:,.2f} (no prior month to compare).\")\n",
    "\n",
    "    # Profit\n",
    "    if profit_change is not None:\n",
    "        lines.append(f\"- **Profit** was {pack['Profit_Trend']} (**{profit_change:+.2f}% MoM**) to **${pack['Profit_Current']:,.2f}**.\")\n",
    "    else:\n",
    "        lines.append(f\"- **Profit**: ${pack['Profit_Current']:,.2f} (no prior month to compare).\")\n",
    "\n",
    "    # Margin\n",
    "    if margin_pt is not None and pack.get(\"Margin_Current_Pct\") is not None:\n",
    "        lines.append(f\"- **Profit margin** was **{pack['Margin_Current_Pct']:.2f}%** ({margin_pt:+.2f} pts vs prior month).\")\n",
    "\n",
    "    # Anomalies\n",
    "    anomalies = []\n",
    "    if pack.get(\"Sales_Anomaly\"):\n",
    "        anomalies.append(\"Sales\")\n",
    "    if pack.get(\"Profit_Anomaly\"):\n",
    "        anomalies.append(\"Profit\")\n",
    "    if anomalies:\n",
    "        lines.append(f\"- ðŸš¨ **Anomaly detected** in: {', '.join(anomalies)} (based on z-score of MoM change).\")\n",
    "\n",
    "    # Drivers\n",
    "    if \"Top_Region_Sales\" in pack:\n",
    "        lines.append(\n",
    "            f\"- **Primary drivers (Sales)**: {pack['Top_Region_Sales']} region (~{pack['Top_Region_Sales_Contribution_Pct']}% of change) \"\n",
    "            f\"and {pack['Top_Category_Sales']} category (~{pack['Top_Category_Sales_Contribution_Pct']}% of change).\"\n",
    "        )\n",
    "\n",
    "    # Orders (context)\n",
    "    if pack.get(\"Orders_Prior\") is not None:\n",
    "        delta_orders = pack[\"Orders_Current\"] - pack[\"Orders_Prior\"]\n",
    "        lines.append(f\"- **Order volume**: {pack['Orders_Current']:,} ({delta_orders:+,} vs prior month).\")\n",
    "\n",
    "    return \"\\n\".join(lines)\n",
    "\n",
    "summary_md = generate_exec_summary(insight_pack)\n",
    "print(summary_md)\n"
   ],
