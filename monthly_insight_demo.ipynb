    "def safe_round(x, nd=2):\n",
    "    try:\n",
    "        return round(float(x), nd)\n",
    "    except Exception:\n",
    "        return None\n",
    "\n",
    "insight_pack = {\n",
    "    \"Month\": current_month,\n",
    "    \"Sales_Current\": safe_round(current_row[\"Total_Sales\"], 2),\n",
    "    \"Sales_Prior\": safe_round(prior_row[\"Total_Sales\"], 2) if prior_row is not None else None,\n",
    "    \"Sales_Change_Pct\": safe_round(current_row[\"Sales_MoM_Pct\"], 2),\n",
    "    \"Sales_Trend\": current_row[\"Total_Sales_Trend\"],\n",
    "    \"Sales_Anomaly\": bool(current_row[\"Total_Sales_Anomaly\"]),\n",
    "\n",
    "    \"Profit_Current\": safe_round(current_row[\"Total_Profit\"], 2),\n",
    "    \"Profit_Prior\": safe_round(prior_row[\"Total_Profit\"], 2) if prior_row is not None else None,\n",
    "    \"Profit_Change_Pct\": safe_round(current_row[\"Profit_MoM_Pct\"], 2),\n",
    "    \"Profit_Trend\": current_row[\"Total_Profit_Trend\"],\n",
    "    \"Profit_Anomaly\": bool(current_row[\"Total_Profit_Anomaly\"]),\n",
    "\n",
    "    \"Margin_Current_Pct\": safe_round(current_row[\"Profit_Margin_Pct\"], 2),\n",
    "    \"Margin_Prior_Pct\": safe_round(prior_row[\"Profit_Margin_Pct\"], 2) if prior_row is not None else None,\n",
    "    \"Margin_Point_Change\": safe_round(current_row[\"Margin_Points_Change\"], 2),\n",
    "\n",
    "    \"Orders_Current\": int(current_row[\"Orders\"]),\n",
    "    \"Orders_Prior\": int(prior_row[\"Orders\"]) if prior_row is not None else None,\n",
    "}\n",
    "\n",
    "# Top drivers (Sales)\n",
    "if previous_month is not None:\n",
    "    top_region_sales = region_sales.head(1).reset_index().iloc[0]\n",
    "    top_category_sales = category_sales.head(1).reset_index().iloc[0]\n",
    "\n",
    "    insight_pack[\"Top_Region_Sales\"] = str(top_region_sales[\"Region\"])\n",
    "    insight_pack[\"Top_Region_Sales_Contribution_Pct\"] = safe_round(top_region_sales[\"contribution_pct\"], 1)\n",
    "\n",
    "    insight_pack[\"Top_Category_Sales\"] = str(top_category_sales[\"Category\"])\n",
    "    insight_pack[\"Top_Category_Sales_Contribution_Pct\"] = safe_round(top_category_sales[\"contribution_pct\"], 1)\n",
    "\n",
    "insight_pack\n"
